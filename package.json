{
    "name": "contextsearch",
    "title": "Context Search",
    "version": "4.5.0",
    "description": "Firefox add-on to search selected text in a web page using your favorite search engines.",
    "browserslist": [
        "Firefox >= 63"
    ],
    "scripts": {
        "test": "eslint .",
        "lint": "eslint .",
        "start": "parcel watch manifest.json --host localhost --config @parcel/config-webextension",
        "build": "parcel build manifest.json --config @parcel/config-webextension"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/odebroqueville/contextSearch.git"
    },
    "keywords": [
        "webextension",
        "context",
        "context search",
        "context menu",
        "search",
        "search engine"
    ],
    "author": "Olivier de Broqueville <olivier.debroqueville@gmail.com>",
    "license": "EULA",
    "bugs": {
        "url": "https://github.com/odebroqueville/contextSearch/issues"
    },
    "homepage": "https://github.com/odebroqueville/contextSearch#readme",
    "dependencies": {
        "@types/sortablejs": "^1.15.1",
        "sortablejs": "^1.15.0"
    },
    "devDependencies": {
        "@parcel/config-webextension": "^2.8.3",
        "eslint": "^8.29.0",
        "parcel": "^2.8.3"
    }
}
